

<h1><%= @place.name %></h1>
<p>Description: <%= @place.description %></p>
<ul>
  <% for place in @places %>
    <li>
      <a href="/places/<%= place.id %>">
        <%= place.name %>
      </a>
    </li>
  <% end %>
</ul>
<p>
  <a href="/places/new">New Contact</a>
</p>
<p>
  <a href="/places">Back to All Places</a>
</p>